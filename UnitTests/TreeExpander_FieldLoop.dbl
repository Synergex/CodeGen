			&	+	"</SEGMENT_LOOP>" + crlf
			&	+	"</PRIMARY_KEY>" + crlf
			&	+	"  - name: a<StructureNoplural>"
			
			expected = 'parameters:' + crlf
			&	+      '  - name: aField1' + crlf
			&	+      '    type: string' + crlf
			&	+      '  - name: aField2' + crlf
			&	+      '    type: integer' + crlf
			&	+      '  - name: aField3' + crlf
			&	+      '    type: number' + crlf
			&	+      '  - name: aUnitTest1'

			;runTest()
			runTest(true)
			
		endmethod		

